.btn--feedback {
    padding: 15px 100px;
    border-radius: 40px 5px;
    background-color: #ffffff;
    background-image: linear-gradient(to right, #9300e8 0%, #4a00ed 50%);
}

.btn__text {
    color: #ffffff;
    font-size: 18px;
    font-weight: 700;
    line-height: 48px;
    text-transform: uppercase;
}

.btn--feedback:hover {
    background-image: linear-gradient(to right, #9300e8 0%, #4a00ed 100%);
}